{
  "name": "@dorgtech/daosmind",
  "version": "0.0.2-alpha2.0",
  "description": "Union of all upctrl packages for alchemy",
  "homepage": "https://uprtcl.io",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "rollup -w -c rollup.config.ts",
    "build": "npm run clean && rollup -c rollup.config.ts",
    "test": "karma start --coverage",
    "test:watch": "karma start --auto-watch=true --single-run=false",
    "package:release": "rm -rf ./package && mkdir ./package && cp -r ./dist/ ./package/dist && cp README.md package.json ./package && cp -r ./node_modules/lit-html ./package/node_modules && cp -r ./node_modules/lit-element ./package/node_modules && cp -r ./node_modules/graphql ./package/node_modules && cp -r ./src ./package/src",
    "release": "npm run build && npm run package:release && npm publish ./package --access public"
  },
  "dependencies": {
    "@uprtcl/access-control": "^0.0.3-alpha3",
    "@uprtcl/cortex": "^0.0.3-alpha3",
    "@uprtcl/documents": "^0.0.3-alpha3",
    "@uprtcl/ethereum-provider": "^0.0.3-alpha3",
    "@uprtcl/evees": "^0.0.3-alpha3",
    "@uprtcl/http-provider": "^0.0.3-alpha3",
    "@uprtcl/ipfs-provider": "^0.0.3-alpha3",
    "@uprtcl/lenses": "^0.0.3-alpha3",
    "@uprtcl/micro-orchestrator": "^0.0.3-alpha3",
    "@uprtcl/multiplatform": "^0.0.3-alpha3",
    "@uprtcl/wikis": "^0.0.3-alpha3",
    "apollo-boost": "^0.4.7",
    "graphql-tag": "^2.10.1",
    "inversify": "^5.0.1",
    "lit-element": "^2.2.1",
    "react": "^16.12.0",
    "redux": "^4.0.4",
    "reflect-metadata": "^0.1.13",
    "rollup": "^1.30.1",
    "rollup-plugin-typescript2": "^0.25.3"
  },
  "devDependencies": {
    "@open-wc/testing": "^0.11.1",
    "@open-wc/testing-karma": "^3.0.0",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-json": "^4.0.1",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@types/lodash": "^4.14.138",
    "@types/node": "^10.11.0",
    "@types/react": "^16.9.17",
    "lodash": "^4.17.15",
    "rimraf": "^2.6.2",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "temp-dir": "^2.0.0",
    "typescript": "^3.7.2"
  },
